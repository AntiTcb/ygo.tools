{
    "name": "ygo.tools",
    "version": "0.3.1",
    "repository": "https://github.com/AntiTcb/ygo.tools",
    "private": true,
    "packageManager": "pnpm@10.28.0",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss .",
        "format:check": "prettier --check --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss .",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:unit": "vitest run src",
        "test:e2e": "playwright test"
    },
    "devDependencies": {
        "@iconify-json/clarity": "^1.2.4",
        "@iconify-json/fa-solid": "^1.2.2",
        "@iconify-json/fa6-regular": "^1.2.4",
        "@iconify-json/heroicons": "^1.2.3",
        "@iconify-json/lucide": "^1.2.85",
        "@iconify-json/mdi": "^1.2.3",
        "@iconify-json/vscode-icons": "^1.2.39",
        "@macfja/svelte-persistent-store": "^2.4.2",
        "@skeletonlabs/skeleton": "3.2.2",
        "@skeletonlabs/skeleton-svelte": "4.9.0",
        "@supabase/supabase-js": "^2.90.1",
        "@sveltejs/adapter-auto": "^6.1.1",
        "@sveltejs/adapter-cloudflare": "^7.2.5",
        "@sveltejs/enhanced-img": "^0.9.2",
        "@sveltejs/kit": "^2.47.2",
        "@sveltejs/vite-plugin-svelte": "^6.2.4",
        "@tailwindcss/forms": "^0.5.11",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.18",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.15.14",
        "@types/nprogress": "^0.2.3",
        "autoprefixer": "^10.4.23",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-svelte": "^3.14.0",
        "globals": "^16.5.0",
        "nprogress": "^0.2.0",
        "prettier": "^3.8.0",
        "prettier-plugin-svelte": "^3.4.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "runed": "^0.37.1",
        "sk-seo": "^0.5.4",
        "super-sitemap": "^1.0.6",
        "svelte": "^5.46.4",
        "svelte-check": "^4.3.5",
        "svelte-search": "^2.1.2",
        "svelte-sonner": "^1.0.7",
        "tailwindcss": "^4.1.18",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0",
        "unplugin-icons": "^22.5.0",
        "vite": "^7.1.11",
        "vite-plugin-mkcert": "^1.17.9",
        "vitest": "^4.0.18"
    },
    "type": "module"
